<table>
  <tr>
    <th>Year</th>
    <th># of Buys</th>
    <th>S&P 500</th>
    <th>Mean ROI</th>
    <th>Median</th>
    <th>STDDEV</th>
    <th>Sharpe</th>
  </tr>
  <% years.keys.sort.reverse.each do |year| %>
    <% data = years[year] %>
    <tr>
      <td>
        <a href="/files/stock_recs.<%= data[:year] %>.html"><%= data[:year] %></a>
      </td>
      <td><%= data[:num] %></td>
      <td><%= (data[:spy] * 100).round 3 %>%</td>
      <td><%= (data[:mean] * 100).round 3 %>%</td>
      <td><%= (data[:median] * 100).round 3 %>%</td>
      <td><%= (data[:stddev] * 100).round 3 %>%</td>
      <td><%= data[:sharpe].round 3 %></td>
    </tr>
  <% end %>
</table>

